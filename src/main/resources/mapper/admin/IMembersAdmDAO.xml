<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.routy.routyback.mapper.admin.IMembersAdmDAO">

    <!-- 전체 회원 조회 -->
    <select id="listAllMembers" resultType="map">
        SELECT
            USERNO,
            USERID,
            USERNAME,
            USERHP,
            USEREMAIL,
            USERLEVEL,
            USERSTATUS,
            USERREGDATE
        FROM USERS
        ORDER BY USERREGDATE DESC
    </select>

</mapper>
